<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.UserMapper">
 <insert id="insertUser" parameterType="com.example.demo.model.SysUser">
    insert into sysuser
    values
    (username,password,name,age,sex)
    (#{username},#{password},#{name},#{age},#{gender}))
 </insert>
 <select id="insertUsername" parameterType="java.lang.String" resultType="com.example.demo.model.SysUser">
  selet*from sysuer
  where
  username=#{username}
 </select>
</mapper>